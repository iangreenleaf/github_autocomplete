{
  "name": "GitHub Autocomplete",
  "version": "0.1.2",
  "description": "Autocompletion for @-mentions on GitHub",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["vendor/jquery.js", "github_autocomplete.user.js"]
    }
  ],
  "permissions": [
    "https://github.com/*"
  ],
  "update_url": "https://github.com/iangreenleaf/github_autocomplete/raw/crx_publish/updates.xml"
}
