{
  "name": "GitHub Autocomplete",
  "version": "0.1.0",
  "description": "Autocompletion for @-mentions on GitHub",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["vendor/jquery.js", "github_autocomplete.user.js"]
    }
  ],
  "permissions": [
    "https://github.com/*"
  ]
}
